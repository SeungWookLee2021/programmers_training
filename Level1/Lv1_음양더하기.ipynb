    "def solution(absolutes, signs):\n",
    "    data = enumerate(absolutes)\n",
    "    real_nums = []\n",
    "    for i, num in data:\n",
    "        if signs[i] == True:\n",
    "            real_nums.append(num)\n",
    "        elif signs[i] == False:\n",
    "            real_nums.append(-num)\n",
    "    answer = sum(real_nums)\n",
    "    return answer"
   ]
  }
